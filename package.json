{
  "name": "eslint-plugin-t",
  "version": "1.5.1",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint --max-warnings 0 \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "format:lib": "prettier --write lib --ignore-path /dev/null",
    "format:check": "prettier --check src",
    "test": "yarn lint && yarn typecheck && yarn format:check",
    "clean": "rm -rf lib",
    "build": "yarn clean && tsc && yarn format:lib",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "^8.4.2",
    "eslint": "^8.17.0",
    "eslint-config-xt": "^1.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "eslintConfig": {
    "extends": "xt"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "repository": "github:eslint-tools/eslint-plugin-t",
  "license": "ISC"
}

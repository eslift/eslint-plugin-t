{
  "name": "eslint-plugin-t",
  "version": "1.5.0",
  "main": "./src/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.js\"",
    "test": "yarn format:check && yarn lint"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-config-xt": "^1.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "eslintConfig": {
    "extends": "xt",
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "repository": "github:eslint-tools/eslint-plugin-t",
  "license": "ISC"
}

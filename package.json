{
  "name": "eslint-plugin-t",
  "version": "1.3.0",
  "repository": "github:eslift/eslint-plugin-t",
  "main": "./src/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.js\"",
    "test": "yarn format:check && yarn lint"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-eslift": "^1.3.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  },
  "eslintConfig": {
    "extends": "eslift",
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "license": "ISC"
}
